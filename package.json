{
  "name": "zabbix-cli-ts",
  "version": "1.0.5-rc.6",
  "description": "This is a typescript lib to make calls to the zabbix api",
  "main": "dist/cjs/index.js",
  "module": "./dist/esm5/index.js",
  "es2015": "./dist/esm2015/index.js",
  "typings": "./dist/typings/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run build:cjs && npm run build:es5 && npm run build:es2015 && npm run build:types",
    "build:cjs": "tsc && tsc-alias",
    "build:es5": "tsc -p tsconfig.esm5.json && tsc-alias -p tsconfig.esm5.json",
    "build:es2015": "tsc -p tsconfig.esm2015.json && tsc-alias -p tsconfig.esm2015.json",
    "build:types": "tsc -p tsconfig.types.json && tsc-alias -p tsconfig.types.json",
    "prettier:check": "npx prettier --check ./src",
    "lint": "npx eslint .",
    "lint:all": "npm run prettier:check && npm run lint",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --passWithNoTests --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:watch": "jest --config ./test/jest-e2e.json --watch",
    "prepare": "husky install",
    "prepublish": "npm run build"
  },
  "keywords": [
    "zabbix",
    "typescript",
    "api"
  ],
  "author": "Flavia Reis Valverde <fareisv@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flaviareisv/zabbix-cli-ts.git"
  },
  "bugs": {
    "url": "https://github.com/flaviareisv/zabbix-cli-ts/issues"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "3.1.1",
    "rimraf": "^5.0.5",
    "semantic-release": "^22.0.12",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": "20.x.x"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
